<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="header.inner.right">
            <block name="support-link"
                   class="Magento\Framework\View\Element\Template"
                   template="Omnyfy_Vendor::support-link.phtml"
                   after="-"/>
        </referenceContainer>

        <move element="support-link" destination="header.inner.right" after="-" />

        <referenceBlock name="logo">
            <arguments>
                <argument name="template" xsi:type="string">Omnyfy_Vendor::page/header.phtml</argument>
            </arguments>
        </referenceBlock>
    </body>
</page>
