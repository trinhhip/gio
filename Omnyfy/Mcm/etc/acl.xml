<?xml version="1.0" ?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Omnyfy_Mcm::base" sortOrder="20" title="Marketplace Commercials Management">
                    <resource id="Omnyfy_Mcm::fees" sortOrder="10" title="Marketplace Fees and Charges Management"/>
                    <resource id="Omnyfy_Mcm::vendor_earning" sortOrder="20" title="Vendor Earnings" />
                    <resource id="Omnyfy_Mcm::marketplace_earning" sortOrder="30" title="Marketplace Earnings"/>
                    <resource id="Omnyfy_Mcm::payouts" sortOrder="40" title="Payout Management">
                        <resource id="Omnyfy_Mcm::pending_payouts" sortOrder="10" title="Pending Payouts"/>
                        <resource id="Omnyfy_Mcm::payout_history" sortOrder="20" title="Payout History"/>
                        <resource id="Omnyfy_Mcm::select_payout_type" sortOrder="30" title="Select Payout Type"/>
                    </resource>

                    <resource id="Omnyfy_Mcm::vendor_payout_history" sortOrder="40" title="Vendor Payout History"/>
                </resource>

                <resource id="Omnyfy_Mcm::report" title="Reports" translate="title" sortOrder="40">
                    <resource id="Omnyfy_Mcm::omnymart_reports" title="Omnymart Reports" translate="title" sortOrder="100">
                        <resource id="Omnyfy_Mcm::marketplace_detailedearning_reports" title="Marketplace Fees Detailed Earnings report" translate="title" sortOrder="10" />
                        <resource id="Omnyfy_Mcm::marketplace_summaryearning_reports" title="Marketplace Fees Summary Earnings report" translate="title" sortOrder="20" />
                        <resource id="Omnyfy_Mcm::vendor_fee_reports" title="Vendor Fees report" translate="title" sortOrder="30" />
                        <resource id="Omnyfy_Mcm::category_commission_reports" title="Category Commissions report" translate="title" sortOrder="40" />
                    </resource>
                </resource>

                <resource id="Magento_Sales::sales" title="Sales" translate="title" sortOrder="20">
                    <resource id="Magento_Sales::sales_operation" title="Operations" translate="title" sortOrder="10">
                        <resource id="Magento_Sales::sales_order"  title="Orders" translate="title" sortOrder="10">
                            <resource id="Magento_Sales::actions" title="Actions" translate="title" sortOrder="10">
                                <resource id="Omnyfy_Mcm::pick_list" title="Pick Slip" translate="title" sortOrder="160" />
                            </resource>
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
