<?xml version="1.0" ?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <title>Price On Request</title>
    </head>
    <body>
        <referenceBlock name="page.main.title">
            <action method="setPageTitle">
                <argument translate="true" name="title" xsi:type="string">Price On Request</argument>
            </action>
        </referenceBlock>
        <referenceContainer name="content">
            <block name="product.quote" class="OmnyfyCustomzation\PriceToQuote\Block\Product\Quote" template="OmnyfyCustomzation_PriceToQuote::product/quote.phtml"/>
        </referenceContainer>
    </body>
</page>
